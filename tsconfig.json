{
	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"strict": false,
		"sourceMap": true,

		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"noImplicitThis": true,


		"esModuleInterop": true,
		"resolveJsonModule": true,

		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
